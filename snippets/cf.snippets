#Script based snippets
snippet comp
	component extends="${1}" accessors="${2:true}" output="${3:false}" {
		${4}
	}
snippet prop
	property name="${1}" type"${2}" setter="${3:true}" getter="${4:true}"${5};
snippet func
	/**
	* ${5:hint}
	*/
	${1:public} ${2:void} function (${3:args}) output="${4:false}" {
		${6}
	}
